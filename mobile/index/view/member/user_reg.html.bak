<script type="text/javascript">

    $(function(){
        $('#regid').click(function(){
            $.ajax({
                url:"/api.php/member/reg",
                type:"post",
                dataType:"json",
                data: $('#form-member-add').serialize(),
                success: function(data) {
                    if(data.status==1){
                        layer.msg(data.msg,{time:2000},function(){
                            window.location.href='/mobile.php/member/pay_annual_free';
                        });
                    }else{
                        layer.msg(data.msg);
                    }
                }
            });
        });
